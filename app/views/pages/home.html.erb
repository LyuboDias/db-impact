  <div class="container" id="contacts">
      <h2 class="services-bigtext">Contact Us</h2>
      <div class="pt-3">
        <form
          action="https://formspree.io/f/maylbwra"
          method="POST"
          class=" text-center form pt-4"
        >
          <label>
                Full Name:<br>
                <input type="text" name="name" class="input">
              </label>
          <label>
            Email:<br>
            <input type="text" name="_replyto" class="input">
          </label>
          <label>
            Your message:<br>
            <textarea name="message" class="text-area"></textarea>
          </label>

          <!-- your other form fields go here -->

          <button type="submit" class="btn blue">Send</button>
        </form>
      </div>
    </div>
  <div  class="contacts">



